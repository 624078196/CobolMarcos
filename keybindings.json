// 将键绑定放在此文件中以覆盖默认值
[
    {
      "key": "ctrl+1",
      "command": "macros.addKome",
      "when": "editorLangId == 'COBOL'"
    },
    {
      "key": "ctrl+2",
      "command": "macros.addDate",
      "when": "editorLangId == 'COBOL'"
    },
    {
      "key": "ctrl+3",
      "command": "insertDateString.insertDate",
      "when": "editorLangId == 'COBOL'"
    },
]